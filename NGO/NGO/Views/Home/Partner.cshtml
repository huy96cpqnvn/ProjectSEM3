@model ListViewModel
<div class="hero-wrap" style="background-image: url('../images/bg_1.jpg');" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center" data-scrollax-parent="true">
            <div class="col-md-7 ftco-animate text-center" data-scrollax=" properties: { translateY: '70%' }">
                <p class="breadcrumbs" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }"><span class="mr-2"><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></span> <span>Partner</span></p>
                <h1 class="mb-3 bread" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Partners</h1>
            </div>
        </div>
    </div>
</div>
<section class="ftco-section">
    <div class="container">
        <div class="row">
            @foreach (var p in Model.Ngos)
            {
                <partial name="PartnerSummary" model="p" />
            }

            </div>
        <div class="row mt-5">
            <div class="col text-center">
                <div class="block-27">
                    <div page-model="@Model.PageInfo" page-action="Partner" page-classes-enabled="true"
                         page-class="btn" page-class-normal="btn-outline-dark"
                         page-class-selected="btn-primary"
                         class="btn-group pull-right m-1">

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>