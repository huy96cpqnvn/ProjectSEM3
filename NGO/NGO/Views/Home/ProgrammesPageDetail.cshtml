<div class="hero-wrap" style="background-image: url('../images/bg_2.jpg');" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center" data-scrollax-parent="true">
            <div class="col-md-7 ftco-animate text-center" data-scrollax=" properties: { translateY: '70%' }">
                <p class="breadcrumbs" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }"><span class="mr-2"><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></span> <span><a asp-area="" asp-controller="Home" asp-action="Causes">Our Project</a></span> <span>Project Detail</span></p>
                <h1 class="mb-3 bread" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Project Detail</h1>
            </div>
        </div>
    </div>
</div>
@model ListViewModel

@foreach (var p in Model.Programmes)
{
    <partial name="ProgramFullSummary" model="p" />
}

